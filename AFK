local function UDOAG_fake_script() -- _1.LocalScript 
	local script = Instance.new('LocalScript', _1)
 
	local Stuff = script.Parent
 
	while true do
		wait()
		script.Parent.TextTransparency = 1
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
			script.Parent.Text = "Anti AFK"
		end
		wait(1)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
		end
		wait(0.5)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
			script.Parent.Text = "M To Toggle GUI"
		end
		wait(1)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
		end
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
			script.Parent.Text = "Anti AFK"
		end
		wait(0.5)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
		end
		wait(1)
 
		wait()
		script.Parent.TextTransparency = 1
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
			script.Parent.Text = "M To Toggle GUI"
		end
		wait(1)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
		end
		wait(0.5)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
			script.Parent.Text = "Anti AFK"
		end
		wait(1)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
		end
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency - 0.1
			script.Parent.Text = "M To Toggle GUI"
		end
		wait(0.5)
		for i = 1,10 do
			wait()
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.1
		end
		wait(1)
	end
end
coroutine.wrap(UDOAG_fake_script)()
local function MKAUTW_fake_script() -- Main.DragifySec 
	local script = Instance.new('LocalScript', Main)
 
	local UserInputService = game:GetService("UserInputService")
 
	local gui = script.Parent
 
	local dragging
	local dragInput
	local dragStart
	local startPos
 
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
 
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
 
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
 
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
 
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(MKAUTW_fake_script)()
local function AQOY_fake_script() -- Main.Toggle 
	local script = Instance.new('LocalScript', Main)
 
	local Main = script.Parent.Parent.Main
	local mouse = game:GetService('Players').LocalPlayer:GetMouse(); 
 
	mouse.KeyDown:connect(function(key) -- whenever a key is pushed down
		key = key:lower() -- i do this in all my key events, though i don't remember why
		if key == 'm' then -- if key pressed is h
			Main.Visible = not Main.Visible
		end
	end)
 
wait(10)
	print ("Toggle Key Is M.")
end
coroutine.wrap(AQOY_fake_script)()
local function MOFHPM_fake_script() -- Main.ANTIAFK 
	local script = Instance.new('LocalScript', Main)
 
	local bb = game:service'VirtualUser'
 
 
	game:service'Players'.LocalPlayer.Idled:connect(function()
		bb:CaptureController()bb:ClickButton2(Vector2.new())
	end)
end
coroutine.wrap(MOFHPM_fake_script)()
local function ZHTW_fake_script() -- Main.Notifs 
	local script = Instance.new('LocalScript', Main)
 
	game.StarterGui:SetCore("SendNotification", {
		Title = "Braiden#0001";
		Text = "Anti AFK Started!";
		Duration = 2;
	})
 
	wait(6)
 
	game.StarterGui:SetCore("SendNotification", {
		Title = "Braiden#0001";
		Text = "Copied Discord To Clipboard, Please Join!";
		Duration = 2;
	})
end
coroutine.wrap(ZHTW_fake_script)()
